{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build",
      "type": "process",
      "command": "xcodebuild",
      "args": [
        "-configuration",
        "Debug",
        "-sdk",
        "${command:ios-debug.targetSdk}",
        "-allowProvisioningUpdates"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
